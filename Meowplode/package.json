    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^2.8.2",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jest-webpack-alias": "^3.3.3",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "ts-jest": "^20.0.6",
