    "react-key-handler": "^0.3.0",
    "react-keydown": "^1.7.3",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
